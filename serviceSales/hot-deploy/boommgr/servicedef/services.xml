<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>BoomMgr Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service>



    <service name="addEmp" engine="java"
             use-transaction="true" require-new-transaction="true" transaction-timeout="9980000"
             location="main.java.com.banfftech.boom.BoomServices"
             invoke="addEmp" auth="false">
        <description>addEmp</description>
        <attribute name="tel" type="String" mode="IN" optional="false"/>
        <attribute name="partyGroupId" type="String" mode="IN" optional="false"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT"
                   optional="true"/>
    </service>






    <service name="createMyLead" engine="java"
             use-transaction="true" require-new-transaction="true" transaction-timeout="9980000"
             location="main.java.com.banfftech.boom.BoomServices"
             invoke="createMyLead" auth="false">
        <description>createMyLead</description>
        <attribute name="supplierName" type="String" mode="IN" optional="false"/>
        <attribute name="supplierTel" type="String" mode="IN" optional="false"/>
        <attribute name="provinceName" type="String" mode="IN" optional="false"/>
        <attribute name="cityName" type="String" mode="IN" optional="false"/>
        <attribute name="countyName" type="String" mode="IN" optional="false"/>
        <attribute name="detailInfo" type="String" mode="IN" optional="false"/>


        <attribute name="leadInfo" type="Map" mode="OUT" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT"
                   optional="true"/>
    </service>

    <service name="createRawMaterials" engine="java"
             use-transaction="true" require-new-transaction="true" transaction-timeout="9980000"
             location="main.java.com.banfftech.boom.BoomServices"
