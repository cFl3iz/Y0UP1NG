        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>

    <service name="register" engine="java"
             use-transaction="true" require-new-transaction="true" transaction-timeout="9980000"
             location="main.java.com.banfftech.boom.BoomServices"
             invoke="register" auth="false">
        <description>register</description>

        <attribute name="openId" type="String" mode="IN" optional="true"/>
        <attribute name="tel" type="String" mode="IN" optional="true"/>
        <attribute name="captcha" type="String" mode="IN" optional="true"/>
        <attribute name="nickName" type="String" mode="IN" optional="true"/>
