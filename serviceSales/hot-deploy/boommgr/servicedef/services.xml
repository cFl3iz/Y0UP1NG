             invoke="boomUserLogin" auth="false">
        <description>boomUserLogin</description>
        <attribute name="openId" type="String" mode="INOUT" optional="true"/>
        <attribute name="tarjeta" type="String" mode="OUT" optional="true"/>
        <attribute name="partyId" type="String" mode="OUT" optional="true"/>
        <attribute name="userInfo" type="Map" mode="OUT" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <service name="empJoinPartyGroup" engine="java"
             use-transaction="true" require-new-transaction="true" transaction-timeout="9980000"
             location="main.java.com.banfftech.boom.BoomServices"
