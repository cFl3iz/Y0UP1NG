<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of PersonManager Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>


    <entity entity-name="PromoEmp" package-name="com.banfftech.personerp"
            title="PromoEmp">

        <description>PromoEmp</description>
        <field name="promoCodeId" type="id-ne"/>
        <field name="partyId" type="id-ne"/>
        <field name="statusId" type="id-ne"/>
        <field name="description" type="value"/>
        <relation type="one" fk-name="PEMP_STATUS_FK" rel-entity-name="StatusItem">
        <key-map field-name="statusId"/>
        </relation>
        <prim-key field="promoCodeId"/>
    </entity>


    <entity entity-name="Roster" package-name="com.banfftech.personerp"
            title="Roster">

        <description>Roster</description>

        <field name="partyId" type="id-ne"></field>

        <field name="rosters" type="value"></field>


        <prim-key field="partyId" />

    </entity>



</entitymodel>