    <!-- SSMS Property -->
    <SystemProperty systemResourceId="pe" systemPropertyId="sms.appkey" systemPropertyValue="23691976" description="短信平台appkey"/>
    <SystemProperty systemResourceId="pe" systemPropertyId="sms.intervalTime" systemPropertyValue="20" description="第二次与第一次短信发送间隔时间"/>
