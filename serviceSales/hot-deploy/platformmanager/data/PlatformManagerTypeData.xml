    <PartyIdentificationType createdStamp="2017-05-11 15:22:41.0" createdTxStamp="2017-05-11 15:22:41.0" lastUpdatedStamp="2017-05-11 15:22:41.0" lastUpdatedTxStamp="2017-05-11 15:22:41.0" partyIdentificationTypeId="JPUSH_ANDROID"/>



