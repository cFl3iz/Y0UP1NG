    <!--<GeoAssoc geoId="EMS06" geoIdTo="EMS_AREAS" geoAssocTypeId="GROUP_MEMBER"/>-->
    <!--<GeoAssoc geoId="EMS07" geoIdTo="EMS_AREAS" geoAssocTypeId="GROUP_MEMBER"/>-->
    <!--<GeoAssoc geoId="EMS08" geoIdTo="EMS_AREAS" geoAssocTypeId="GROUP_MEMBER"/>-->
    <!--<GeoAssoc geoId="EMS09" geoIdTo="EMS_AREAS" geoAssocTypeId="GROUP_MEMBER"/>-->

    <ProductStoreShipmentMeth productStoreShipMethId="CP_EMS" productStoreId="9000" shipmentMethodTypeId="EMS" partyId="CHINAPOST" roleTypeId="CARRIER"
                              includeGeoId="EMS_AREAS" />
    <!--这个includeGeoId就指明了EMS只适用存在于"EMS_AREAS"这个GROUP内的国家。如果不在的话，网店上将不会出现EMS这种运输方式。-->





