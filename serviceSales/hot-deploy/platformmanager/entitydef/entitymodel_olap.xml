    <entity entity-name="DkAgentForwardChainFact" package-name="org.apache.ofbiz.olap" title="DkAgentForwardChainFact">

        <field name="basePartyId" type="id-ne"/>
        <field name="basePartyName" type="value"/>
        <field name="baseStoreName" type="value"/>
        <field name="baseStoreId" type="value"/>


        <field name="partyIdFrom" type="id-ne"/>
        <field name="partyIdTo" type="id-ne"/>

        <field name="firstName" type="value"/>
        <field name="objectInfo" type="value"/>

        <field name="productId" type="value"/>
