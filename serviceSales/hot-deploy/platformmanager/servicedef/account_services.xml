<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>PlatformManager Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service>

    <service name="registerDk" engine="java"
             use-transaction="true" require-new-transaction="true" transaction-timeout="9980000"
             location="main.java.com.banfftech.personmanager.PersonManagerServices"
             invoke="registerDk" auth="false">
        <description>registerDk</description>

        <attribute name="openId" type="String" mode="IN" optional="true"/>
        <attribute name="appId" type="String" mode="IN" optional="true"/>
        <attribute name="tel" type="String" mode="IN" optional="true"/>
        <attribute name="captcha" type="String" mode="IN" optional="true"/>
        <attribute name="nickName" type="String" mode="IN" optional="true"/>
        <attribute name="name" type="String" mode="IN" optional="true"/>
        <attribute name="gender" type="String" mode="IN" optional="true"/>
        <attribute name="language" type="String" mode="IN" optional="true"/>
        <attribute name="avatarUrl" type="String" mode="IN" optional="true"/>
        <attribute name="city" type="String" mode="IN" optional="true"/>
        <attribute name="country" type="String" mode="IN" optional="true"/>
        <attribute name="province" type="String" mode="IN" optional="true"/>


        <attribute name="partyId" type="String" mode="OUT" optional="true"/>
        <attribute name="tarjeta" type="String" mode="OUT" optional="true"/>
        <attribute name="userInfo" type="Map" mode="OUT" optional="true"/>
        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>
    <!-- App Login -->
    <service name="userAppLogin" engine="java"
             location="main.java.com.banfftech.platformmanager.common.PlatformLoginWorker" invoke="userAppLogin" auth="false">
        <description>app Login</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="uuid" type="String" mode="IN" optional="true"/>
        <attribute name="openId" type="String" mode="IN" optional="true"/>

        <attribute name="captcha" type="String" mode="IN" optional="false"/>
        <attribute name="tarjeta" type="String" mode="OUT" optional="true"/>
        <attribute name="partyId" type="String" mode="OUT" optional="true"/>
        <attribute name="newUser" type="String" mode="OUT" optional="true"/>

        <attribute name="resultMap" type="java.util.Map" mode="OUT" optional="true"/>
    </service>




