    <!-- accessWeChatFromBuyProduct -->
    <request-map uri="accessWeChatFromBuyProduct">
        <security auth="false" https="false"/>
        <event type="java" path="main.java.com.banfftech.webmanager.WebServices"
               invoke="accessWeChatFromBuyProduct"/>

        <response name="success" type="request-redirect" value="shareProduct">
            <redirect-parameter name="tarjeta" from="tarjeta"/>
            <redirect-parameter name="spm" from="spm"/>
            <redirect-parameter name="returnPayToPartyId" from="returnPayToPartyId"/>
            <redirect-parameter name="subscribe" from="subscribe"/>
            <redirect-parameter name="productId" from="productId"/>
            <redirect-parameter name="nowPersonName" value="${nowPersonName}"/>
        </response>

        <response name="error" type="request-redirect" value="shareProduct">
            <redirect-parameter name="productId" from="productId"/>
        </response>
    </request-map>



    <request-map uri="accessWeChatFromBuyStory">
        <security auth="false" https="false"/>
        <event type="java" path="main.java.com.banfftech.webmanager.WebServices"
               invoke="accessWeChatFromBuyStory"/>

        <response name="success" type="request-redirect" value="myStory">
            <redirect-parameter name="tarjeta" from="tarjeta"/>
            <redirect-parameter name="subscribe" from="subscribe"/>
            <redirect-parameter name="spm" from="spm"/>

            <redirect-parameter name="partyId" from="partyId"/>
            <redirect-parameter name="productId" from="productId"/>
        </response>

        <response name="error" type="request-redirect" value="myStory">
            <redirect-parameter name="productId" from="productId"/>
        </response>
    </request-map>


